{
  "name": "i18n-auto-translation",
  "version": "1.2.2",
  "description": "Auto translate i18n JSON file(s) to desired language(s).",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/while1618/i18n-auto-translation",
  "bin": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-updates": "npx npm-check-updates"
  },
  "keywords": [
    "i18n",
    "auto translate",
    "translation",
    "google translate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/while1618/i18n-auto-translation.git"
  },
  "dependencies": {
    "@google-cloud/translate": "7.2.0",
    "axios": "1.3.3",
    "deep-object-diff": "1.1.9",
    "glob": "8.1.0",
    "html-entities": "2.3.3",
    "just-extend": "6.2.0",
    "yargs": "17.7.0"
  },
  "devDependencies": {
    "@types/glob": "8.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.0",
    "@types/yargs": "17.0.22",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
