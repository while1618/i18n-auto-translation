{
  "name": "i18n-auto-translation",
  "version": "1.1.15",
  "description": "Auto translate i18n JSON file(s) to desired language(s).",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/while1618/i18n-auto-translation",
  "bin": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
